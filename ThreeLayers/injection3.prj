            </properties>
        </medium>
        <medium id="10">
          <phases>
                <phase>
                    <type>Gas</type>
                    <properties>
                        <property>
                            <name>viscosity</name>
                            <type>Constant</type>
                            <value>1.0e-3</value>
                        </property>
                        <property>
                            <name>density</name>
                            <type>Constant</type>
                            <value>1000</value>
                        </property>
                    </properties>
                </phase>
                <phase>
                    <type>Solid</type>
                    <properties>
                        <property>
                            <name>density</name>
                            <type>Constant</type>
                            <value>2250</value>
                        </property>
                    </properties>
                </phase>
            </phases>
           <properties>
                <property>
                    <name>porosity</name>
                    <type>Constant</type>
                    <value>0.08</value>
                </property>
                <property>
                    <name>biot_coefficient</name>
                    <type>Constant</type>
                    <value>0.7</value>
                </property>
                <property>
                    <name>reference_temperature</name>
                    <type>Constant</type>
                    <value>293.15</value>
                </property>
                <property>
                    <name>permeability</name>
                    <type>Constant</type>
                    <value>1e-18</value>
                </property>
            </properties>
        </medium>
    </media>
    <parameters>
        <parameter>
            <name>YoungModulus</name>
            <type>Group</type>
            <group_id_property>MaterialIDs</group_id_property>
            <index_values>
                <index>7</index>
                <value>18.66e9</value> <!--Pa-->
            </index_values>
            <index_values>
                <index>8</index>
                <value>25.0e9</value> <!--Pa-->
            </index_values>
            <index_values>
                <index>9</index>
                <value>25.0e9</value> <!--Pa-->
            </index_values>
            <index_values>
                <index>10</index>
                <value>18.66e9</value> <!--Pa-->
            </index_values>
        </parameter>
        <parameter>
            <name>PoissonRatio</name>
            <type>Group</type>
            <group_id_property>MaterialIDs</group_id_property>
            <index_values>
                <index>7</index>
                <value>0.25</value>
            </index_values>
            <index_values>
                <index>8</index>
                <value>0.33</value>
            </index_values>
            <index_values>
                <index>9</index>
                <value>0.33</value>
            </index_values>
            <index_values>
                <index>10</index>
                <value>0.25</value>
            </index_values>
        </parameter>
        <parameter>
            <name>displacement0</name>
            <type>Constant</type>
            <values>0 0 0</values>
        </parameter>
        <parameter>
            <name>pressure0</name>
            <type>Constant</type>
            <values>0</values>
        </parameter>
        <parameter>
            <name>zero</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>
        <parameter>
            <name>nodal_flux</name>
            <type>Constant</type>
            <!-- in kg/m^-3 /sec -->
            <!-- <value>0.001</value> -->
            <!-- 5 kg/s divided by 40000 m^2 area -->
            <!-- <value>0.000125</value> -->
            <!-- 5 kg/s divided by 2016000 m^3 volume -->
            <value>0.00000062</value>
            <!-- A arbitary number -->
            <!-- <value>1e-10</value> -->
        </parameter>
        <parameter>
            <name>no_flux</name>
            <type>Constant</type>
            <value>0</value>
            <!-- in kg/sec -->
            <!-- <value>0.000</value> -->
        </parameter>
        <parameter>
            <name>TimeDependentSourceTerm</name>
            <type>TimeDependentHeterogeneousParameter</type>
            <time_series>
                <pair>
                    <time>0</time>
                    <parameter_name>no_flux</parameter_name>
                </pair>
                <pair>
                    <time>864000</time>
                    <!-- start inject since the 10th day -->
                    <parameter_name>no_flux</parameter_name>
                </pair>
                <pair>
                    <time>2592000</time>
                    <!-- stop inject at the 30th day -->
                    <parameter_name>nodal_flux</parameter_name>
                </pair>
                <pair>
                    <time>8640000</time>
                    <!-- keep no inject at the 30th day -->
                    <parameter_name>no_flux</parameter_name>
                </pair>
            </time_series>
        </parameter>
    </parameters>
    <process_variables>
        <process_variable>
            <name>displacement</name>
            <components>3</components>
            <order>2</order>
            <initial_condition>displacement0</initial_condition>
            <boundary_conditions>
                <boundary_condition>
                    <mesh>injectionmesh3_physical_group_Front</mesh>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>zero</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>injectionmesh3_physical_group_Back</mesh>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>zero</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>injectionmesh3_physical_group_Right</mesh>
                    <type>Dirichlet</type>
                    <component>1</component>
                    <parameter>zero</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>injectionmesh3_physical_group_Left</mesh>
                    <type>Dirichlet</type>
                    <component>1</component>
                    <parameter>zero</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>injectionmesh3_physical_group_Ceiling</mesh>
                    <type>Dirichlet</type>
                    <component>2</component>
                    <parameter>zero</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>injectionmesh3_physical_group_Floor</mesh>
                    <type>Dirichlet</type>
                    <component>2</component>
                    <parameter>zero</parameter>
                </boundary_condition>
            </boundary_conditions>
        </process_variable>
        <process_variable>
            <name>pressure</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>pressure0</initial_condition>
            <boundary_conditions>  
                <boundary_condition>
                    <mesh>injectionmesh3_physical_group_Right</mesh>
                    <type>Neumann</type>
                    <component>0</component>
                    <parameter>pressure0</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>injectionmesh3_physical_group_Left</mesh>
                    <type>Neumann</type>
                    <component>0</component>
                    <parameter>pressure0</parameter>
                </boundary_condition>
                 <boundary_condition>
                    <mesh>injectionmesh3_physical_group_Front</mesh>
                    <type>Neumann</type>
                    <component>0</component>
                    <parameter>pressure0</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>injectionmesh3_physical_group_Back</mesh>
                    <type>Neumann</type>
                    <component>0</component>
                    <parameter>pressure0</parameter>
                </boundary_condition>
            </boundary_conditions>
            <source_terms>
                <source_term>
                    <mesh>injectionmesh3_physical_group_Injection</mesh>
                    <!-- <type>Nodal</type> -->
                    <type>Volumetric</type>
                    <parameter>TimeDependentSourceTerm</parameter>
                </source_term>
            </source_terms>
        </process_variable>
    </process_variables>
    <nonlinear_solvers>
        <nonlinear_solver>
            <name>basic_newton</name>
            <type>Newton</type>
            <max_iter>50</max_iter>
            <linear_solver>general_linear_solver</linear_solver>
        </nonlinear_solver>
    </nonlinear_solvers>
    <linear_solvers>
        <linear_solver>
            <name>general_linear_solver</name>
            <lis>-i bicgstab -p ilu -tol 1e-16 -maxiter 10000</lis>
            <eigen>
                <solver_type>BiCGSTAB</solver_type>
                <precon_type>ILUT</precon_type>
                <max_iteration_step>10000</max_iteration_step>
                <error_tolerance>1e-10</error_tolerance>
            </eigen>
        </linear_solver>
    </linear_solvers>
</OpenGeoSysProject>
